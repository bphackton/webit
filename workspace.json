{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/express"
  },
  "defaultProject": "webit-node",
  "projects": {
    "webit-node": {
      "root": "apps/webit-node",
      "sourceRoot": "apps\\webit-node\\src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/webit-node",
            "main": "apps/webit-node/src/main.ts",
            "tsConfig": "apps/webit-node/tsconfig.app.json",
            "assets": [
              "apps/webit-node/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/webit-node/src/environments/environment.ts",
                  "with": "apps/webit-node/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "webit-node:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/webit-node/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/webit-node"
          ],
          "options": {
            "jestConfig": "apps/webit-node/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "webit-auth-node": {
      "root": "apps/webit-auth-node",
      "sourceRoot": "apps\\webit-auth-node\\src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/webit-auth-node",
            "main": "apps/webit-auth-node/src/main.ts",
            "tsConfig": "apps/webit-auth-node/tsconfig.app.json",
            "assets": [
              "apps/webit-auth-node/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/webit-auth-node/src/environments/environment.ts",
                  "with": "apps/webit-auth-node/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "webit-auth-node:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/webit-auth-node/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/webit-auth-node"
          ],
          "options": {
            "jestConfig": "apps/webit-auth-node/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}